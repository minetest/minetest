msgid "Liquid fluidity smoothing"
msgstr "Pelembutan kebendaliran cecair"

#: src/settings_translation_file.cpp
msgid "Liquid sink"
msgstr "Lubuk cecair"

#: src/settings_translation_file.cpp
msgid "Gravity"
msgstr "Graviti"

